{
  "name": "boxdeliverybackend",
  "version": "1.1.0",
  "description": "This is the Backend repository of the Box Delivery page.",
  "main": "server.ts",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint . --quiet && echo 'Linting completed without errors'",
    "start": "nodemon server.ts",
    "build": "tsc",
    "test": "jest --colors --verbose --silent --detectOpenHandles",
    "prepare": "husky install",
    "generate-swagger": "swagger-jsdoc -o ./build/swagger.json ./src/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-merge": "npm run premerge",
      "post-merge": "npm run version"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FabiolaLutrario/BoxDeliveryBackend.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/FabiolaLutrario/BoxDeliveryBackend#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "semver": "^7.5.4",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2"
